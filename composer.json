{
    "name": "prometee/vies-client",
    "description": "European VAT Information Exchange System SOAP client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "prometee@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-soap": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1",
        "symplify/easy-coding-standard": "^8|^9|^10",
        "phpunit/phpunit": "^8|^9"
    },
    "autoload": {
        "psr-4": { "Prometee\\VIESClient\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Prometee\\VIESClient\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
